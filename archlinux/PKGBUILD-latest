pkgname=python2-sinpy
pkgver=20131006181028
pkgrel=1
pkgdesc="SinPy is a micro WSGI framework."
arch=('any')
url="http://wrutschkow.org/sinpy"
license=('LGPL')
depends=('python2' 'python2-six')

pkgver() {
    date +%Y%m%d%H%M%S
}

package() {
    cd ../../
    python2 setup.py install --root="$pkgdir/" --optimize=1
}
